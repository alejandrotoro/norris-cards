= form_for @usuario_cartum do |f|
  -if @usuario_cartum.errors.any?
    #error_explanation
      %h2= "#{pluralize(@usuario_cartum.errors.count, "error")} prohibited this usuario_cartum from being saved:"
      %ul
        - @usuario_cartum.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :partida_id
    = f.text_field :partida_id
  .field
    = f.label :usuario_id
    = f.text_field :usuario_id
  .field
    = f.label :carta_id
    = f.text_field :carta_id
  .field
    = f.label :orden
    = f.number_field :orden
  .actions
    = f.submit 'Save'
